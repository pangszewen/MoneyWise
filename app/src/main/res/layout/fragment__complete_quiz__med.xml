<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment_CompleteQuiz_Med">

    <TextView
        android:id="@+id/congrats_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Keep it up!"
        android:textSize="30dp"
        android:textColor="@color/maroon"
        android:fontFamily="@font/donegal_one"
        android:textStyle="bold"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="10dp"/>

    <TextView
        android:id="@+id/message_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="You're doing well!"
        android:textSize="23dp"
        android:textColor="@color/blue"
        android:fontFamily="@font/donegal_one"
        android:textStyle="bold"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="50dp"/>

    <TextView
        android:id="@+id/your_score_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Your Score: "
        android:textSize="28dp"
        android:textColor="@color/dark_yellow"
        android:fontFamily="@font/donegal_one"
        android:textStyle="bold"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="150dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="%"
        android:textSize="45dp"
        android:textColor="@color/dark_yellow"
        android:textStyle="bold"
        android:layout_marginTop="190dp"
        android:layout_gravity="center_horizontal"
        >
    </TextView>

    <ImageView
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:src="@drawable/good_work"
        android:layout_marginTop="270dp"
        android:layout_gravity="center_horizontal">
    </ImageView>

</FrameLayout>